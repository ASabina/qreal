                        </linePort>
                        <linePort type="NonTyped">
                            <start startx="10" starty="99"/>
                            <end endx="139" endy="100"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property name="elementName" displayedName="Name of Element" type="string"/>
                        <property name="textCode" displayedName="text code" type="string"/>
                    </properties>
                    <explodesTo/>
                </logic>
            </node>
            <node name="AbstractElement">
                <logic>
                    <container/>
