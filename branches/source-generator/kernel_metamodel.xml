<?xml version="1.0" encoding="utf-8" ?>
<metamodel xmlns="http://schema.real.com/schema/">
	<!-- Kernel UML 2.0 package -->
	<namespace>UML 2.1</namespace>
	<diagram name="Kernel">
		<enumType name="State" id="krntState">
			<enumValue>Created</enumValue>
			<enumValue>Development</enumValue>
			<enumValue>Ready for testing</enumValue>
			<enumValue>Testing</enumValue>
			<enumValue>Feedback</enumValue>
			<enumValue>Debugging</enumValue>
			<enumValue>Ready for acceptance</enumValue>
			<enumValue>Accepted</enumValue>
			<enumValue>Release</enumValue>
			<enumValue>Launched</enumValue>
		</enumType>
		<enumType name="VisibilityKind" id="krntVisibility">
			<enumValue>private</enumValue>
			<enumValue>protected</enumValue>
			<enumValue>public</enumValue>
			<enumValue>package</enumValue>
		</enumType>
		<enumType name="ParameterDirectionKind" id="krntParameterDirection">
			<enumValue>in</enumValue>
			<enumValue>inout</enumValue>
			<enumValue>out</enumValue>
			<enumValue>return</enumValue>
		</enumType>
		<!-- The main element -->
		<node name="Element" id="krnnElement">
			<logic>
				<properties>
					<property name="state" type="enum">
						<enum idref="krntState" />
						<default>Created</default>
					</property>
					<!--property name="comment" type="string"/-->
				</properties>
				<containers>
					<container name="E-E">
						<contains idref="krnnElement" role="ownedElement" />
					</container>
					<container name="E-E">
						<contained_by idref="krnnElement" role="owner" />
					</container>
				</containers>
				<associations>
					<assoc_ref idref="krnaRelEl1" />
					<assoc_ref idref="krnaRelEl2" />
					<assoc_ref idref="krnaDirrelSrc" />
					<assoc_ref idref="krnaDirrelTarg" />
					<assoc_ref idref="krnaComLinkElem" />
				</associations>
			</logic>
		</node>
		<node name="Diagram" id="krnnDiagram">
			<graphics>
				<view>
					<svg_shape>
						<svg:svg width="30" height="30" version="1.1" xmlns:svg="http://www.w3.org/2000/svg">
						<rect x="0" y="0" width="30" style="stroke:#000000;fill:#f8f8f8;fill-opacity:0;" rx="1.0" height="20" id="0"/>
                        </svg:svg>
					</svg_shape>
					<repo_info>
						<html:html xmlns:html="http://www.w3.org/html/">
							<html:center>
								<html:b>
									<html:text_from_repo name="name" />
								</html:b>
							</html:center>
						</html:html>
					</repo_info>
					<ports>
						<point_port x="0" y="10" />
						<point_port x="15" y="0" />
						<point_port x="30" y="10" />
						<point_port x="15" y="20" />
					</ports>
				</view>
			</graphics>
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnNamedElement" />
					</generalization>
				</generalizations>
				<properties>
					<property name="language" type="string" />
				</properties>
			</logic>
		</node>
		<!-- Named Element -->
		<node name="NamedElement" id="krnnNamedElement">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnElement" />
					</generalization>
				</generalizations>
				<properties>
					<property name="name" type="string" />
					<property name="visibility" type="enum">
						<enum idref="krntVisibility" />
					</property>
					<property name="qualifiedName" type="string" />
				</properties>
				<containers>
					<container name="NE-C">
						<contained_by idref="krnnClassifier" />
					</container>
					<container name="N-NE_ownship">
						<contained_by idref="krnnNamespace" role="ownedMember"></contained_by>
					</container>
					<container name="N-NE_membership">
						<contained_by idref="krnnNamespace" role="member"></contained_by>
					</container>
				</containers>
			</logic>
		</node>
		<!-- General relationship -->
		<edge name="Relationship" id="krneRelationship">
			<logic>
				<associations>
					<association id="krnaRelEl1" role="relatedElement" end_type="no_arrow">
						<begin idref="krnnElement" />
					</association>
					<association id="krnaRelEl2" role="relatedElement" end_type="no_arrow">
						<end idref="krnnElement" />
					</association>
				</associations>
				<generalizations>
					<generalization>
						<parent parent_id="krnnElement" />
					</generalization>
				</generalizations>
			</logic>
		</edge>
		<!-- Directed relationship -->
		<edge name="DirectedRelationship" id="krneDirRelationship">
			<logic>
				<properties>
					<property name="condition" type="string" />
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="krneRelationship" />
					</generalization>
				</generalizations>
				<associations>
					<association id="krnaDirrelSrc" role="source" end_type="no_arrow">
						<begin idref="krnnElement" />
					</association>
					<association id="krnaDirrelTarg" role="target"  end_type="open_arrow">
						<end idref="krnnElement" />
					</association>
				</associations>
			</logic>
		</edge>
		<!-- Comment node and link -->
		<node name="Comment" id="krnnComment">
			<logic>
				<properties>
					<property name="body" type="string" />
				</properties>
				<associations>
					<assoc_ref idref="krnaComLinkCom" />
				</associations>
				<generalizations>
					<generalization>
						<parent parent_id="krnnElement" />
					</generalization>
				</generalizations>
				<containers>
					<container name="E-Com">
						<contained_by idref="krnnElement" role="ownedComment" />
					</container>
				</containers>
			</logic>
		</node>
		<edge name="CommentLink" id="krneComLink">
			<logic>
				<associations>
					<association id="krnaComLinkCom">
						<begin idref="krnnComment" />
					</association>
					<association id="krnaComLinkElem" role="annotatedElement">
						<end idref="krnnElement" />
					</association>
				</associations>
			</logic>
		</edge>
		<!-- Namespace part -->
		<node name="Namespace" id="krnnNamespace">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnNamedElement" />
					</generalization>
				</generalizations>
				<containers>
					<container name="N-NE_ownship">
						<contains idref="krnnNamedElement" role="Namespace" min="0" max="1"></contains>
					</container>
					<container name="N-NE_membership">
						<contains idref="krnnNamedElement"></contains>
					</container>
					<container name="N-PE">
						<contains idref="krnnPackageableElem"></contains>
					</container>
				</containers>
				<associations>
					<assoc_ref idref="krnaElementImportNamespace"></assoc_ref>
					<assoc_ref idref="krnaPackageImportNamespace"></assoc_ref>
				</associations>
			</logic>
		</node>
		<edge id="krneElementImport" name="ElementImport">
			<logic>
				<properties>
					<property name="alias" type="string"></property>
					<property name="visibility" type="enum">
						<enum idref="krntVisibility" />
					</property>
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="krneDirRelationship" />
					</generalization>
				</generalizations>
				<associations>
					<association id="krnaElementImportNamespace" role="elementImport">
						<begin idref="krnnNamespace"></begin>
					</association>
					<association id="krnaElementImportPackageableElement">
						<end idref="krnnPackageableElem"></end>
					</association>
				</associations>
			</logic>
		</edge>
		<edge id="krnePackageImport" name="PackageImport">
			<logic>
				<properties>
					<property name="visibility" type="enum">
						<enum idref="krntVisibility" />
					</property>
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="krneDirRelationship" />
					</generalization>
				</generalizations>
				<associations>
					<association id="krnaPackageImportNamespace" role="packageImport">
						<begin idref="krnnNamespace"></begin>
					</association>
					<association id="krnaPackageImportPackage">
						<end idref="krnnPackage"></end>
					</association>
				</associations>
			</logic>
		</edge>
		<node id="krnnPackage" name="Package">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnNamespace"></parent>
					</generalization>
					<generalization>
						<parent parent_id="krnnPackageableElem"></parent>
					</generalization>
				</generalizations>
				<associations>
					<assoc_ref idref="krnaElementImportPackageableElement"></assoc_ref>
				</associations>
			</logic>
		</node>
		<!-- Types part -->
		<node name="PackageableElement" id="krnnPackageableElem">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnNamedElement" />
					</generalization>
				</generalizations>
				<associations>
					<assoc_ref idref="krnaElementImportPackageableElement"></assoc_ref>
				</associations>
			</logic>
		</node>
		<node name="Type" id="krnnType">
			<logic>
				<properties>
					<property name="visibility" type="enum">
						<enum idref="krntVisibility" />
					</property>
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="krnnPackageableElem" />
					</generalization>
				</generalizations>
				<containers>
					<container name="Type-Op">
						<contained_by idref="krnnOperation" />
					</container>
					<container name="Type-BF">
						<contained_by idref="krnnBehavioralFeature" />
					</container>
				</containers>
			</logic>
		</node>
		<node name="TypedElement" id="krnnTypedElem">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnNamedElement" />
					</generalization>
				</generalizations>
				<properties>
					<property name="type" type="ref">
						<ref idref="krnnType" />
					</property>
				</properties>
			</logic>
		</node>
		<!-- Classifier part -->
		<node name="RedefinableElement" id="krnnRedefElement">
			<logic>
				<properties>
					<property name="isLeaf" type="bool" />
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="krnnNamedElement" />
					</generalization>
				</generalizations>
				<containers>
					<container name="RE-C">
						<contains idref="krnnClassifier"></contains>
					</container>
					<container name="RE-RE">
						<contains idref="krnnRedefElement"></contains>
					</container>
					<container name="RE-RE">
						<contained_by idref="krnnRedefElement" role="redefinedElement"></contained_by>
					</container>
				</containers>
			</logic>
		</node>
		<node name="Classifier" id="krnnClassifier">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnType" />
					</generalization>
					<generalization>
						<parent parent_id="krnnNamespace" />
					</generalization>
					<generalization>
						<parent parent_id="krnnRedefElement" />
					</generalization>
				</generalizations>
				<properties>
					<property name="isAbstract" type="bool">
						<default>false</default>
					</property>
				</properties>
				<containers>
					<container name="Classifier-Property">
						<contains idref="krnnProperty" min="0" unbounded="true" role="attribute" />
					</container>
					<container name="Classifier-Operation">
						<contains idref="krnnOperation" min="0" unbounded="true" role="operation" />
					</container>
					<container name="Classifier-Feature">
						<contains idref="krnnFeature" min="0" unbounded="true" role="feature" />
					</container>
					<container name="Classifier-NamedElement">
						<contains idref="krnnNamedElement" min="0" unbounded="true" role="inheritedMember" />
					</container>
					<container name="Classifier-Classifier">
						<contains idref="krnnClassifier" min="0" unbounded="true" role="general" />
					</container>
					<container name="Classifier-Classifier">
						<contained_by idref="krnnClassifier" />
					</container>
					<container name="Class-Classifier">
						<contained_by idref="cnClass" role="nestedClassifier" />
					</container>
					<container name="Classifier-RE">
						<contained_by idref="krnnRedefElement" role="redefinableContext"></contained_by>
					</container>
				</containers>
				<associations>
					<assoc_ref idref="krnaGereralEnd" />
					<assoc_ref idref="krnaGereralStart" />
				</associations>
			</logic>
		</node>
		<node name="Property" id="krnnProperty">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnStructuralFeature" />
					</generalization>
				</generalizations>
				<containers>
					<container name="Classifier-Property">
						<contained_by idref="krnnClassifier" role="classifier" />
					</container>
				</containers>
			</logic>
		</node>
		<edge name="Generalization" id="krneGeneralization">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krneDirRelationship" />
					</generalization>
				</generalizations>
				<properties>
					<property name="isSubstitutable" type="bool" />
				</properties>
				<associations>
					<association id="krnaGeneralEnd" role="general" end_type="empty_arrow">
						<end idref="krnnClassifier" />
					</association>
					<association id="krnaGeneralStart">
						<begin idref="krnnClassifier" />
					</association>
				</associations>
			</logic>
		</edge>
		<!-- Operations part -->
		<node name="Operation" id="krnnOperation">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnBehavioralFeature" />
					</generalization>
				</generalizations>
				<properties>
					<property name="isQuery" type="bool">
						<default>false</default>
					</property>
					<property name="isOrdered" type="bool" />
					<property name="isUnique" type="bool" />
					<property name="lower" type="int" />
					<property name="upper" type="positiveInt" />
					<property name="type" type="ref">
						<ref idref="krnnType" />
					</property>
				</properties>
				<containers>
					<container name="Classifier-Operation">
						<contained_by idref="krnnClassifier" role="operationingClassifier" />
					</container>
					<container name="Operation-Type">
						<contains idref="krnnType" min="0" unbounded="true" role="raisedException" />
					</container>
					<container name="Operation-Prameter">
						<contains idref="krnnParameter" min="0" unbounded="true" role="ownedParameter" />
					</container>
				</containers>
			</logic>
		</node>
		<node name="Parameter" id="krnnParameter">
			<logic>
				<properties>
					<property name="direction" type="enum">
						<enum idref="krntParameterDirection" />
					</property>
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="krnnTypedElem" />
					</generalization>
				</generalizations>
				<containers>
					<container name="Operation-Parameter">
						<contained_by idref="krnnOperation" role="operation" />
					</container>
					<container name="BF-Parameter">
						<contained_by idref="krnnBehavioralFeature" role="ownerFormalParam" />
					</container>
					<container name="Behaviour-Parameter">
						<contained_by idref="bbnBehavior" role="ownedParameter"></contained_by>
					</container>
				</containers>
			</logic>
		</node>
		<!-- Features diagram of the Kernel package -->
		<node name="Feature" id="krnnFeature">
			<logic>
				<properties>
					<property name="isStatic" type="bool" />
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="krnnRedefElement" />
					</generalization>
				</generalizations>
				<containers>
					<container name="Classifier-Feature">
						<contained_by idref="krnnClassifier" role="featuringClassifier" />
					</container>
				</containers>
			</logic>
		</node>
		<node id="krnnBehavioralFeature" name="BehavioralFeature">
			<logic>
				<properties>
					<property name="isAbstract" type="bool"></property>
				</properties>
				<containers>
					<container name="BF-Type">
						<contains idref="krnnType" min="0" unbounded="true" role="raisedException" />
					</container>
					<container name="BF-Parameter">
						<contains idref="krnnParameter" min="0" unbounded="true" role="ownedParameter" />
					</container>
					<container name="Behavior-BF">
						<contained_by idref="bbnBehavior" role="specification"></contained_by>
					</container>
				</containers>
				<generalizations>
					<generalization>
						<parent parent_id="krnnFeature" />
					</generalization>
					<generalization>
						<parent parent_id="krnnNamespace"></parent>
					</generalization>
				</generalizations>
			</logic>
		</node>
		<node id="krnnStructuralFeature" name="StructuralFeature">
			<logic>
				<properties>
					<property name="isReadOnly" type="bool" />
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="krnnFeature" />
					</generalization>
					<generalization>
						<parent parent_id="krnnTypedElem" />
					</generalization>
				</generalizations>
			</logic>
		</node>
	</diagram>
</metamodel>
