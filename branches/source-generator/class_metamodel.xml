<?xml version="1.0" encoding="utf-8" ?>
<metamodel xmlns="http://schema.real.com/schema/">
	<include>
		kernel_metamodel
	</include>
	<namespace>UML 2.1</namespace>
	<diagram name="CLASS DIAGRAM">
		<node name="Class" id="cnClass">
			<graphics>
				<view>
					<svg_shape>
						<svg:svg width="50" height="70" version="1.1" xmlns:svg="http://www.w3.org/2000/svg">
							<defs>
								<linearGradient id="class_gradient" x1="0%" y1="0%" x2="100%" y2="100%">
									<stop offset="0%" style="stop-color:rgb(127,127,127);stop-opacity:1"/>
									<stop offset="40%" style="stop-color:rgb(255,255,255);stop-opacity:1"/>
									<stop offset="80%" style="stop-color:rgb(255,255,255);stop-opacity:1"/>
									<stop offset="100%" style="stop-color:rgb(127,127,127);stop-opacity:1"/>
								</linearGradient>
							</defs>
						    <rect x="0" y="0" width="50" style="stroke:#000000;fill:url(#class_gradient)" height="70"/>
						</svg:svg>
					</svg_shape>
					<repo_info>
						<html:html xmlns:html="http://www.w3.org/html/">
							<html:b>
                                <html:i>
                                    <html:center>
								        <html:text_from_repo name="name" />
                                    </html:center>    
                                </html:i>    
							</html:b>
                            <html:hr/>
                            <html:b>
                                <html:text_from_repo name="vals"/>
                            </html:b>    
                            <html:hr/>
                            <html:b>
                                <html:text_from_repo name="methods"/>
                            </html:b>    
						</html:html>
					</repo_info>
                    <ports>
                        <line_port>
                            <start startx="0" starty="0"/>
                            <end endx="50" endy="0"/>
                        </line_port>
                        <line_port>
                            <start startx="50" starty="0"/>
                            <end endx="50" endy="70"/>
                        </line_port>
                        <line_port>
                            <start startx="50" starty="70"/>
                            <end endx="0" endy="70"/>
                        </line_port>
                        <line_port>
                            <start startx="0" starty="70"/>
                            <end endx="0" endy="0"/>
                        </line_port>

                    </ports>
				</view>
			</graphics>
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnClassifier" />
					</generalization>
				</generalizations>
                <properties>
					<property name="vals" type="string" />
					<property name="methods" type="string" />
					<property name="logicalName" type="string" />
					<property name="type" type="string" />
                </properties>
				<containers>
					<container name="Class-Classifier">
						<contains idref="krnnClassifier" role="class"></contains>
					</container>
				</containers>
			</logic>
		</node>
		<node name="View" id="cnClassView">
			<graphics>
				<view>
					<svg_shape>
					<svg:svg contentScriptType="text/ecmascript" width="100"
					          zoomAndPan="magnify"   contentStyleType="text/css" viewBox="0 0 100 100" height="100" width="100"
										          preserveAspectRatio="xMidYMid meet"   xmlns="http://www.w3.org/2000/svg" version="1.0">
          <svg:text x="20" y="10" font-size="15" fill="blue">View</svg:text>
          <svg:line x1="0" y1="20" x2="100" y2="20" stroke="green" stroke-width="2" />
          <svg:line x1="0" y1="100" x2="100" y2="100" stroke="green" stroke-width="2" />
          <svg:line x1="0" y1="20" x2="0" y2="100" stroke="green" stroke-width="2" />
          <svg:line x1="100" y1="20" x2="100" y2="100" stroke="green" stroke-width="2" />
          </svg:svg>
					</svg_shape>
					<repo_info>
						<html:html xmlns:html="http://www.w3.org/html/">
							<html:b>
                                <html:i>
                                    <html:center>
								        <html:text_from_repo name="name" />
                                    </html:center>    
                                </html:i>    
							</html:b>
						</html:html>
					</repo_info>
                    <ports>
                        <line_port>
                            <start startx="0" starty="20"/>
                            <end endx="100" endy="20"/>
                        </line_port>
                        <line_port>
                            <start startx="0" starty="100"/>
                            <end endx="100" endy="100"/>
                        </line_port>
                        <line_port>
                            <start startx="0" starty="20"/>
                            <end endx="0" endy="100"/>
                        </line_port>
                        <line_port>
                            <start startx="100" starty="20"/>
                            <end endx="100" endy="100"/>
                        </line_port>

                    </ports>
				</view>
			</graphics>
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnClassifier" />
					</generalization>
				</generalizations>
                <properties>
					<property name="logicalName" type="string" />
                </properties>
				<containers>
					<container name="Class-Classifier">
						<contains idref="krnnClassifier" role="class"></contains>
					</container>
				</containers>
			</logic>
		</node>
		<edge name="Aggregation" id="ceAggregation">
			<logic>
				<associations>
					<association id="caAggr1" end_type="empty_rhomb">
						<begin idref="krnnElement" />
					</association>
					<association id="acAggr2" end_type="no_arrow">
						<end idref="krnnElement" />
					</association>
				</associations>
				<generalizations>
					<generalization>
						<parent parent_id="krneRelationship" />
					</generalization>
				</generalizations>
			</logic>
		</edge>
		<edge name="Relation" id="ceRelation">
			<logic>
				<associations>
					<association id="caAggr1" end_type="no_arrow">
						<begin idref="krnnElement" />
					</association>
					<association id="acAggr2" end_type="no_arrow">
						<end idref="krnnElement" />
					</association>
				</associations>
				<generalizations>
					<generalization>
						<parent parent_id="krneRelationship" />
					</generalization>
				</generalizations>
<!--				<properties>
					<property name="fromMultiplicity" type="string" />
					<property name="toMultiplicity" type="string" />
				</properties>								-->
			</logic>
		</edge>
		<edge name="Composition" id="ceComposition">
			<logic>
				<associations>
					<association id="caComp1" end_type="filled_rhomb">
						<begin idref="krnnElement" />
					</association>
					<association id="acComp2" end_type="no_arrow">
						<end idref="krnnElement" />
					</association>
				</associations>
				<generalizations>
					<generalization>
						<parent parent_id="krneRelationship" />
					</generalization>
				</generalizations>
			</logic>
		</edge>
		<edge name="Dependency" id="ceDependency">
            <graphics>
                <view>
                    <line_type type="dashLine"/>
                </view>
            </graphics>
			<logic>
				<associations>
					<association id="caDep1" end_type="no_arrow">
						<begin idref="krnnElement" />
					</association>
					<association id="acAggr2" end_type="open_arrow">
						<end idref="krnnElement" />
					</association>
				</associations>
				<generalizations>
					<generalization>
						<parent parent_id="krneRelationship" />
					</generalization>
				</generalizations>
			</logic>
		</edge>



        
	</diagram>
</metamodel>
