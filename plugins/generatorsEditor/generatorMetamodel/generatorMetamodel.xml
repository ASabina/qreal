                        <arc y1="0" y2="79" x1="120" x2="199" spanAngle="1440" startAngle="0"/>
                        <arc y1="120" y2="199" x1="0" x2="79" spanAngle="1440" startAngle="2880"/>
                        <arc y1="120" y2="199" x1="120" x2="199" spanAngle="-1440" startAngle="0"/>
                    </picture>
                    <labels>
                        <label x="30" y="15" textBinded="name"/>
                    </labels>
                    <ports>
                        <linePort type="NonTyped">
                            <start startx="-1" starty="40"/>
                            <end endx="0" endy="159"/>
                        </linePort>
                        <linePort type="NonTyped">
                            <start startx="199" starty="40"/>
                            <end endx="200" endy="159"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <contains type="SemanticNode"/>
                        <contains type="ForeachNode"/>
                        <contains type="ConverterNode"/>
                    </container>
                    <explodesTo/>
                </logic>
            </node>
            <node name="SemanticNode" displayedName="Semantic Node" description="Элемент для задания генерируемого кода">
                <graphics>
                    <picture sizex="164" sizey="101">
