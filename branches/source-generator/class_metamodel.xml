						<parent parent_id="krneRelationship" />
					</generalization>
				</generalizations>
			</logic>
		</edge>
		<edge name="Dependency" id="ceDependency">
            <graphics>
                <view>
                    <line_type type="dashLine"/>
                </view>
            </graphics>
			<logic>
				<associations>
					<association id="caDep1" end_type="no_arrow">
						<begin idref="krnnElement" />
					</association>
					<association id="acAggr2" end_type="open_arrow">
						<end idref="krnnElement" />
					</association>
				</associations>
				<generalizations>
					<generalization>
						<parent parent_id="krneRelationship" />
					</generalization>
				</generalizations>
			</logic>
		</edge>

