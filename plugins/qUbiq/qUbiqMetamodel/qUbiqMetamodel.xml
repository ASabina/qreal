<?xml version="1.0" encoding="utf-8"?>
<metamodel xmlns="http://schema.real.com/schema/">
    <namespace>UML 2.0</namespace>
    <diagram displayedName="QUbiq Logic Editor" nodeName="qUbiqLogicDiagram" name="qUbiqLogicEditor">
        <nonGraphicTypes>
            <enum displayedName="Type of handler" name="handlerType">
                <value>slide handler</value>
                <value>list handler</value>
                <value>button handler</value>
                <value>grid handler</value>
            </enum>
            <enum displayedName="Type of memory" name="takeType">
                <value>only unselected data</value>
                <value>all data</value>
                <value>only selected data</value>
            </enum>
            <enum displayedName="Type of sent message" name="sentMessageType">
                <value>request game</value>
                <value>request of playing field status</value>
                <value>sent a playing field status</value>
                <value>request a players list</value>
            </enum>
            <enum displayedName="Type of wait message" name="waitMessageType">
                <value>response about playing field status</value>
                <value>response to game request</value>
            </enum>
            <enum displayedName="Type of receiver" name="sentToType">
                <value>server</value>
                <value>other player</value>
            </enum>
        </nonGraphicTypes>
        <graphicTypes>
            <node displayedName="Logic Diagram" name="qUbiqLogicDiagram">
                <graphics>
                    <picture sizex="174" sizey="131">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="130" fill-style="none" x2="173"/>
                    </picture>
                    <labels>
                        <label x="7" y="7a" text="Logic Diagram"/>
                        <label x="8" y="34a" textBinded="handlerType"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container>
                        <contains type="go"/>
                        <contains type="message"/>
                        <contains type="take"/>
                        <contains type="waitMessage"/>
                    </container>
                    <properties>
                        <property displayedName="Type of handler" type="handlerType" name="handlerType"/>
                    </properties>
                </logic>
            </node>
            <node displayedName="Take" name="take">
                <graphics>
                    <picture sizex="150" sizey="90">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0a" stroke-width="0" x1="0" y2="89a" fill-style="none" x2="149"/>
                    </picture>
                    <labels>
                        <label x="10a" y="10a" text="TAKE"/>
                        <label x="10a" y="30a" text="from:"/>
                        <label x="10a" y="60a" text="to:"/>
                        <label x="50" y="30a" textBinded="takeFrom"/>
                        <label x="50" y="60a" textBinded="takeTo"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="89a"/>
                            <end endx="139" endy="90a"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1a"/>
                            <end endx="139" endy="0a"/>
                        </linePort>
                        <linePort>
                            <start startx="149" starty="10a"/>
                            <end endx="150" endy="79a"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10a"/>
                            <end endx="0" endy="79a"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="To element ID" type="string" name="takeTo"/>
                        <property displayedName="From element ID" type="string" name="takeFrom"/>
                        <property displayedName="Type of memory" type="takeType" name="takeType">
                            <default>all</default>
                        </property>
                    </properties>
                </logic>
            </node>
            <node displayedName="Go to slide" name="go">
                <graphics>
                    <picture sizex="110" sizey="40">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0a" stroke-width="0" x1="0" y2="39a" fill-style="none" x2="109"/>
                    </picture>
                    <labels>
                        <label x="50a" y="10a" textBinded="goto"/>
                        <label x="10a" y="10a" text="GO to:"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="39a"/>
                            <end endx="99" endy="40a"/>
                        </linePort>
                        <linePort>
                            <start startx="109" starty="9a"/>
                            <end endx="110" endy="10a"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1a"/>
                            <end endx="99" endy="0a"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10a"/>
                            <end endx="0" endy="29a"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Gor to ID slide" type="string" name="goto"/>
                    </properties>
                </logic>
            </node>
            <node displayedName="Sent message" name="message">
                <graphics>
                    <picture sizex="140" sizey="100">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0a" stroke-width="0" x1="0" y2="99a" fill-style="none" x2="139"/>
                    </picture>
                    <labels>
                        <label x="10a" y="10a" text="MESSAGE"/>
                        <label x="10a" y="40a" text="type:"/>
                        <label x="10a" y="70a" text="to:"/>
                        <label x="50" y="40a" textBinded="messageType"/>
                        <label x="50" y="70a" textBinded="sentTo"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="99a"/>
                            <end endx="129" endy="100a"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1a"/>
                            <end endx="129" endy="0a"/>
                        </linePort>
                        <linePort>
                            <start startx="139" starty="10a"/>
                            <end endx="140" endy="89a"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10a"/>
                            <end endx="0" endy="89a"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Type of sent message" type="sentMessageType" name="messageType"/>
                        <property displayedName="Reciever" type="sentToType" name="sentTo"/>
                        <property displayedName="Additional text" type="string" name="additionalText"/>
                    </properties>
                </logic>
            </node>
            <node displayedName="Wait message" name="waitMessage">
                <graphics>
                    <picture sizex="142" sizey="100">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0a" stroke-width="0" x1="0" y2="99a" fill-style="none" x2="139"/>
                    </picture>
                    <labels>
                        <label x="10a" y="10a" text="WAIT MESSAGE"/>
                        <label x="10a" y="40a" text="type:"/>
                        <label x="10a" y="70a" text="from:"/>
                        <label x="50" y="40a" textBinded="waitMessageType"/>
                        <label x="50" y="70a" textBinded="waitFrom"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="99a"/>
                            <end endx="129" endy="100a"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1a"/>
                            <end endx="129" endy="0a"/>
                        </linePort>
                        <linePort>
                            <start startx="139" starty="10a"/>
                            <end endx="140" endy="89a"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10a"/>
                            <end endx="0" endy="89a"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Type of wait message" type="waitMessageType" name="waitMessageType"/>
                        <property displayedName="Sender" type="sentToType" name="waitFrom"/>
                    </properties>
                </logic>
            </node>
        </graphicTypes>
    </diagram>
    <diagram displayedName="QUbiq Presentation Editor" nodeName="qUbiqPresentationDiagram" name="qUbiqPresentationEditor">
        <nonGraphicTypes>
            <enum displayedName="Types of element" name="elementTypes">
                <value>text</value>
                <value>list</value>
                <value>image</value>
                <value>grid</value>
                <value>number</value>
            </enum>
        </nonGraphicTypes>
        <graphicTypes>
            <node displayedName="PresentationDiagram" name="qUbiqPresentationDiagram">
                <graphics>
                    <picture sizex="175" sizey="132">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="131" fill-style="none" x2="174"/>
                    </picture>
                    <labels>
                        <label x="7" y="7a" text="Presentation Diagram"/>
                        <label x="8" y="34a" textBinded="name"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container>
                        <contains type="abstractHandlerNode"/>
                        <contains type="variable"/>
                    </container>
                </logic>
            </node>
            <node displayedName="Slide" name="slide">
                <graphics>
                    <picture sizex="150" sizey="120">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="119" fill-style="none" x2="149"/>
                    </picture>
                    <labels>
                        <label x="10a" y="10a" text="SLIDE"/>
                        <label x="50" y="10a" textBinded="name"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="119"/>
                            <end endx="139" endy="120"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1"/>
                            <end endx="139" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="149" starty="10"/>
                            <end endx="150" endy="109"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="109"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <contains type="abstractHandlerNode"/>
                        <contains type="variable"/>
                    </container>
                    <properties>
                        <property displayedName="Description" type="string" name="description"/>
                    </properties>
                    <generalizations>
                        <parent parentName="abstractHandlerNode"/>
                    </generalizations>
                </logic>
            </node>
            <node displayedName="Button" name="button">
                <graphics>
                    <picture sizex="110" sizey="40">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="39" fill-style="none" x2="109"/>
                    </picture>
                    <labels>
                        <label x="60" y="10a" textBinded="name"/>
                        <label x="10a" y="10a" text="BUTTON"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="39"/>
                            <end endx="99" endy="40"/>
                        </linePort>
                        <linePort>
                            <start startx="109" starty="10"/>
                            <end endx="110" endy="29"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1"/>
                            <end endx="99" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="29"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <contains type="image"/>
                        <contains type="text"/>
                    </container>
                    <properties>
                        <property displayedName="Text on button" type="string" name="buttonText"/>
                    </properties>
                    <generalizations>
                        <parent parentName="abstractHandlerNode"/>
                    </generalizations>
                </logic>
            </node>
            <node displayedName="Exit Button" name="exitButton">
                <graphics>
                    <picture sizex="130" sizey="40">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="39" fill-style="none" x2="129"/>
                    </picture>
                    <labels>
                        <label x="80" y="10a" textBinded="name"/>
                        <label x="10a" y="10a" text="EXIT BUTTON"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="39"/>
                            <end endx="119" endy="40"/>
                        </linePort>
                        <linePort>
                            <start startx="129" starty="10"/>
                            <end endx="130" endy="29"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1"/>
                            <end endx="119" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="29"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <contains type="text"/>
                        <contains type="image"/>
                    </container>
                    <generalizations>
                        <parent parentName="button"/>
                    </generalizations>
                </logic>
            </node>
            <node displayedName="List" name="list">
                <graphics>
                    <picture sizex="110" sizey="40">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="39" fill-style="none" x2="109"/>
                    </picture>
                    <labels>
                        <label x="40" y="10a" textBinded="name"/>
                        <label x="10a" y="10a" text="LIST"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="39"/>
                            <end endx="99" endy="40"/>
                        </linePort>
                        <linePort>
                            <start startx="109" starty="10"/>
                            <end endx="110" endy="29"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1"/>
                            <end endx="99" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="29"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <contains type="text"/>
                        <contains type="image"/>
                        <contains type="button"/>
                    </container>
                    <properties>
                        <property displayedName="Count of elements" type="int" name="elementsCount"/>
                    </properties>
                    <generalizations>
                        <parent parentName="abstractIsNullNode"/>
                    </generalizations>
                </logic>
            </node>
            <node name="abstractHandlerNode">
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Handler" type="string" name="handler"/>
                    </properties>
                </logic>
            </node>
            <node name="abstractIsNullNode">
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Is Null in begin" type="bool" name="isNull">
                            <default>false</default>
                        </property>
                    </properties>
                    <generalizations>
                        <parent parentName="abstractHandlerNode"/>
                    </generalizations>
                </logic>
            </node>
            <node displayedName="Text" name="text">
                <graphics>
                    <picture sizex="110" sizey="40">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="39" fill-style="none" x2="109"/>
                    </picture>
                    <labels>
                        <label x="50" y="10a" textBinded="name"/>
                        <label x="10a" y="10a" text="TEXT"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="39"/>
                            <end endx="99" endy="40"/>
                        </linePort>
                        <linePort>
                            <start startx="109" starty="10"/>
                            <end endx="110" endy="29"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1"/>
                            <end endx="99" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="29"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Text" type="string" name="text"/>
                    </properties>
                    <generalizations>
                        <parent parentName="abstractIsNullNode"/>
                    </generalizations>
                </logic>
            </node>
            <node displayedName="Image" name="image">
                <graphics>
                    <picture sizex="110" sizey="40">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="39" fill-style="none" x2="109"/>
                    </picture>
                    <labels>
                        <label x="60" y="10a" textBinded="name"/>
                        <label x="10a" y="10a" text="IMAGE"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="39"/>
                            <end endx="99" endy="40"/>
                        </linePort>
                        <linePort>
                            <start startx="109" starty="10"/>
                            <end endx="110" endy="29"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1"/>
                            <end endx="99" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="29"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Path to image" type="string" name="pathToImage"/>
                    </properties>
                    <generalizations>
                        <parent parentName="abstractIsNullNode"/>
                    </generalizations>
                </logic>
            </node>
            <node displayedName="Grid" name="grid">
                <graphics>
                    <picture sizex="130" sizey="40">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="39" fill-style="none" x2="129"/>
                    </picture>
                    <labels>
                        <label x="50" y="10a" textBinded="name"/>
                        <label x="90" y="10a" textBinded="size"/>
                        <label x="10a" y="10a" text="GRID"/>
                    </labels>
                    <ports>
                        <linePort>
                            <start startx="10" starty="39"/>
                            <end endx="119" endy="40"/>
                        </linePort>
                        <linePort>
                            <start startx="129" starty="10"/>
                            <end endx="130" endy="29"/>
                        </linePort>
                        <linePort>
                            <start startx="10" starty="-1"/>
                            <end endx="119" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="29"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <contains type="button"/>
                        <contains type="text"/>
                        <contains type="image"/>
                    </container>
                    <properties>
                        <property displayedName="Dimension" type="string" name="size">
                            <default>1x1</default>
                        </property>
                        <property displayedName="Id of element" type="string" name="elementId"/>
                    </properties>
                    <generalizations>
                        <parent parentName="abstractHandlerNode"/>
                    </generalizations>
                </logic>
            </node>
            <node displayedName="Variable" name="variable">
                <graphics>
                    <picture sizex="81" sizey="85">
                        <ellipse fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="84" fill-style="none" x2="80"/>
                    </picture>
                    <labels>
                        <label x="22a" y="16a" textBinded="name"/>
                        <label x="23a" y="47a" textBinded="type"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container/>
                    <properties>
                        <property displayedName="Type of variable" type="elementTypes" name="type">
                            <default>text</default>
                        </property>
                        <property displayedName="Default value" type="string" name="defaultValue"/>
                        <property displayedName="Description" type="string" name="description"/>
                    </properties>
                </logic>
            </node>
            <edge displayedName="Transition Link" name="transitionLink">
                <graphics>
                    <lineType type="dashLine"/>
                </graphics>
                <logic>
                    <associations endType="open_arrow" beginType="no_arrow">
                        <association beginName="begin" endName="end"/>
                    </associations>
                </logic>
            </edge>
        </graphicTypes>
    </diagram>
    <diagram displayedName="QUbiq Condition Editor" nodeName="qUbiqConditionDiagram" name="qUbiqConditionEditor">
        <graphicTypes>
            <node displayedName="Condition Diagram" name="qUbiqConditionDiagram">
                <graphics>
                    <picture sizex="176" sizey="133">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="132" fill-style="none" x2="175"/>
                    </picture>
                    <labels>
                        <label x="7" y="7a" text="Condition Diagram"/>
                        <label x="8" y="34a" textBinded="name"/>
                    </labels>
                    <ports/>
                </graphics>
                <logic>
                    <container/>
                </logic>
            </node>
        </graphicTypes>
    </diagram>
</metamodel>
