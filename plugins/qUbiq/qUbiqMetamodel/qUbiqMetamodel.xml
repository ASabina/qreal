                        <linePort>
                            <start startx="10" starty="-1"/>
                            <end endx="119" endy="0"/>
                        </linePort>
                        <linePort>
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="29"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <contains type="text"/>
                        <contains type="image"/>
                    </container>
                    <generalizations>
                        <parent parentName="button"/>
                    </generalizations>
                </logic>
            </node>
            <node displayedName="List" name="list">
