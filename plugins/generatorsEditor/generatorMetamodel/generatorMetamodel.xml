<?xml version="1.0" encoding="utf-8"?>
<metamodel xmlns="http://schema.real.com/schema/">
    <namespace>UML 2.0</namespace>
    <diagram name="GeneratorEditor" nodeName="GeneratorRoot" displayedName="Generator Editor">
        <nonGraphicTypes>
            <enum name="languageType">
                <value>other</value>
                <value>behavioral</value>
                <value>structural</value>
            </enum>
        </nonGraphicTypes>
        <graphicTypes>
            <node name="GeneratorRoot" displayedName="Generator Root" description="Корневой элемент модели генератора">
                <graphics>
                    <picture sizex="31" sizey="31">
                        <ellipse stroke="#000000" y1="0" y2="29" x1="0" stroke-style="dot" x2="29" stroke-width="1" fill-style="solid" fill="#ffffff"/>
                        <ellipse stroke="#000000" y1="5" y2="24" x1="5" stroke-style="solid" x2="24" stroke-width="1" fill-style="solid" fill="#00f000"/>
                        <text font-size="15" font-name="Arial" y1="6" b="0" x1="10" u="0" font-fill="#000000" i="0">G</text>
                    </picture>
                    <labels/>
                    <ports/>
                </graphics>
                <logic>
                    <container>
                        <contains type="GeneratorDiagram"/>
                    </container>
                    <properties>
                        <property name="toGeneratePath" displayedName="directory to generate" type="string"/>
                        <property name="toQrealPath" displayedName="path to QReal Source" type="string"/>
                        <property name="languageType" displayedName="language type" type="languageType">
                            <default>other</default>
                        </property>
                    </properties>
                    <explodesTo/>
                </logic>
            </node>
            <node name="GeneratorDiagram" displayedName="Generator Diagram" description="Элемент описания части генератора">
