                        </linePort>
                        <linePort type="NonTyped">
                            <start startx="10" starty="99"/>
                            <end endx="139" endy="100"/>
                        </linePort>
                    </ports>
                </graphics>
                <logic>
                    <container>
                        <contains type="SemanticNode"/>
                    </container>
                    <generalizations>
                        <parent parentName="AbstractElement"/>
                    </generalizations>
                    <explodesTo/>
                </logic>
            </node>
