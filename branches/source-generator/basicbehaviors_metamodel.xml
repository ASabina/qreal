<?xml version="1.0" encoding="utf-8" ?>
<metamodel xmlns="http://schema.real.com/schema/">
	<include>kernel_metamodel</include>
	<namespace>UML 2.1</namespace>
	<diagram name="BASIC BEHAVIOURS">
		<!-- BasicBehaviors -->
		<node name="Behavour" id="bbnBehavior">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="cnClass"></parent>
					</generalization>
				</generalizations>
				<containers>
					<container>
						<contains idref="krnnBehavioralFeature" role="method"></contains>
					</container>
					<container>
						<contains idref="krnnParameter"></contains>
					</container>
					<container inherited_from="E-E">
						<contained_by idref="bbnBehavioredClassifier" role="ownedBehavior"></contained_by>
					</container>
				</containers>
			</logic>
		</node>
		<node name="Behavour" id="bbnBehavioredClassifier">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="krnnClassifier"></parent>
					</generalization>
				</generalizations>
			</logic>
		</node>
		<node name="Behavour" id="bbnOpaqueBehavior">
			<logic>
				<properties>
					<property name="body" type="text"></property>
					<property name="language" type="text"></property>
				</properties>
				<generalizations>
					<generalization>
						<parent parent_id="bbnBehavior"></parent>
					</generalization>
				</generalizations>
				<containers>
					<container inherited_from="E-E">
						<contains idref="bbnBehavior"></contains>
					</container>
				</containers>
			</logic>
		</node>
		<node name="Behavour" id="bbnFunctionBehavior">
			<logic>
				<generalizations>
					<generalization>
						<parent parent_id="bbnOpaqueBehavior"></parent>
					</generalization>
				</generalizations>
			</logic>
		</node>
	</diagram>
</metamodel>
