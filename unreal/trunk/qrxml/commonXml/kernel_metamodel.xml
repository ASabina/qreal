<?xml version='1.0' encoding='utf-8'?>
<metamodel xmlns="http://schema.real.com/schema/">
	<namespace>UML 2.1</namespace>
	<diagram name="Kernel">
		<nonGraphicTypes>
			<enum name="Kernel Diagram::State">
				<value>Created</value>
				<value>Development</value>
				<value>Ready for testing</value>
				<value>Testing</value>
				<value>Feedback</value>
				<value>Debugging</value>
				<value>Ready for acceptance</value>
				<value>Accepted</value>
				<value>Release</value>
				<value>Launched</value>
			</enum>
			<enum name="Kernel Diagram::Visibility Kind">
				<value>private</value>
				<value>protected</value>
				<value>public</value>
				<value>package</value>
			</enum>
			<enum name="Kernel Diagram::Parameter Direction Kind">
				<value>in</value>
				<value>inout</value>
				<value>out</value>
				<value>return</value>
			</enum>
		</nonGraphicTypes>
		<graphicTypes>
			<node name="Kernel Diagram::Kernel Element">
				<logic>
					<properties>
						<property type="enum" name="state">
							<enum nameReference="State"/>
							<default>Created</default>
						</property>
					</properties>
				</logic>
			</node>
			<node name="Kernel Diagram::Kernel Diagram">
				<graphics>
					<picture sizex="200" sizey="200">
						<line x1="20" y1="0" x2="180" y2="0"/>
						<line x1="20" y1="200" x2="180" y2="200"/>
						<line x1="0" y1="20" x2="0" y2="180"/>
						<line x1="200" y1="20" x2="200" y2="180"/>
						<arc x1="0" y1="0" x2="40" y2="40" startAngle="1440" spanAngle="1440"/>
						<arc x1="160" y1="0" x2="40" y2="40" startAngle="0" spanAngle="1440"/>
						<arc x1="0" y1="160" x2="40" y2="40" startAngle="2880" spanAngle="1440"/>
						<arc x1="160" y1="160" x2="40" y2="40" startAngle="0" spanAngle="-1440"/>
					</picture>
					<labels>
						<label x="40" y="10" textBinded="name"/>
					</labels>
					<ports>
						<pointPort x="0" y="100"/>
						<pointPort x="100" y="0"/>
						<pointPort x="200" y="100"/>
						<pointPort x="100" y="200"/>
					</ports>
				</graphics>
				<logic>
					<properties>
						<property type="string" name="language"/>
						<property type="bool" name="isView"/>
					</properties>
				</logic>
			</node>
			<node name="Kernel Diagram::Named Element">
				<logic>
					<generalizations>
						<parent parentName="Kernel Diagram::Kernel Element"/>
					</generalizations>
					<properties>
						<property type="string" name="name"/>
						<property type="enum" name="visibility">
							<enum nameReference="Kernel Diagram::Visibility Kind"/>
						</property>
						<property type="string" name="qualifiedName"/>
					</properties>
				</logic>
			</node>
			<edge name="Kernel Diagram::Kernel Relationship">
				<logic>
					<properties>
						<!-- TO DISCUSS!!! -->
						<property type="string" name="from"/>
						<property type="string" name="to"/>
						<property type="string" name="fromPort"/>
						<property type="string" name="toPort"/>
						<!-- END TO DISCUSS!!! -->
						<property type="string" name="fromMultiplicity"/>
						<property type="string" name="toMultiplicity"/>
					</properties>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Diagram::Kernel Element"
									 endName="Kernel Diagram::Kernel Element"/>
					</associations>
				</logic>
			</edge>
			<edge name="Kernel Diagram::Kernel Directed Relationship">
				<logic>
					<properties>
						<property type="string" name="condition"/>
					</properties>
					<associations beginType="no_arrow" endType="open_arrow">
						<association beginName="Kernel Diagram::Kernel Element"
									 endName="Kernel Diagram::Kernel Element"/>
					</associations>
				</logic>
			</edge>
			<node name="Kernel Diagram::Comment">
				<logic>
					<properties>
						<property type="string" name="body"/>
					</properties>
					<generalizations>
						<parent parentName="Kernel Diagram::Kernel Element"/>
					</generalizations>
				</logic>
			</node>
			<edge name="Kernel Diagram::Comment Link">
				<logic>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Diagram::Kernel Element"
									 endName="Kernel Diagram::Kernel Element"/>
					</associations>
				</logic>
			</edge>
			<edge name="Kernel Diagram::Element Import">
				<logic>
					<generalizations>
						<parent parentName="Kernel Diagram::Kernel Directed Relationship"/>
					</generalizations>
					<properties>
						<property type="string" name="alias"/>
					</properties>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Diagram::Kernel Element"
									 endName="Kernel Diagram::Kernel Element"/>
					</associations>
				</logic>
			</edge>
			<edge name="Kernel Diagram::Package Import">
				<logic>
					<generalizations>
						<parent parentName="Kernel Diagram::Kernel Directed Relationship"/>
					</generalizations>
					<associations beginType="no_arrow" endType="no_arrow">
						<association beginName="Kernel Diagram::Kernel Element"
									 endName="Kernel Diagram::Kernel Element"/>
					</associations>
				</logic>
			</edge>
			<node name="Kernel Diagram::Redefinable Element">
				<logic>
					<properties>
						<property type="bool" name="isLeaf"/>
					</properties>
				</logic>
			</node>
			<node name="Kernel Diagram::Classifier">
				<logic>
					<generalizations>
						<parent parentName="Kernel Diagram::Redefinable Element"/>
					</generalizations>
					<properties>
						<property type="bool" name="isAbstract">
							<default>false</default>
						</property>
					</properties>
				</logic>
			</node>
			<edge name="Kernel Diagram::Kernel Generalization">
				<logic>
					<generalizations>
						<parent parentName="Kernel Diagram::Kernel Directed Relationship"/>
					</generalizations>
					<properties>
						<property type="bool" name="isSubstitutable"/>
					</properties>
					<associations beginType="no_arrow" endType="empty_arrow">
						<association beginName="Kernel Diagram::Kernel Element"
									 endName="Kernel Diagram::Kernel Element"/>
					</associations>
				</logic>
			</edge>
			<node name="Kernel Diagram::Feature">
				<logic>
					<generalizations>
						<parent parentName="Kernel Diagram::Redefinable Element"/>
					</generalizations>
					<properties>
						<property type="bool" name="isStatic"/>
					</properties>
				</logic>
			</node>
			<node name="Kernel Diagram::Behavioral Feature">
				<logic>
					<generalizations>
						<parent parentFeature="Kernel Diagram::Feature"/>
					</generalizations>
					<properties>
						<property type="bool" name="isAbstract"/>
					</properties>
				</logic>
			</node>
			<node name="Kernel Diagram::Structural Feature">
				<logic>
					<generalizations>
						<parent parentFeature="Kernel Diagram::Feature"/>
					</generalizations>
					<properties>
						<property type="bool" name="isReadOnly"/>
					</properties>
				</logic>
			</node>
			<node name="Kernel Diagram::Operation">
				<logic>
					<generalizations>
						<parent parentName="Kernel Diagram::Behavioral Feature"/>
					</generalizations>
					<properties>
						<property type="bool" name="isQuery">
							<default>false</default>
						</property>
						<property type="bool" name="isOrdered"/>
						<property type="bool" name="isUnique"/>
						<property type="int" name="lower"/>
						<property type="positiveInt" name="upper"/>
					</properties>
				</logic>
			</node>
			<node name="Kernel Diagram::Parameter">
				<logic>
					<properties>
						<property type="enum" name="direction">
							<enum nameReference="Kernel Diagram::Parameter Direction Kind"/>
						</property>
					</properties>
				</logic>
			</node>
		
		</graphicTypes>
	</diagram>
</metamodel>
