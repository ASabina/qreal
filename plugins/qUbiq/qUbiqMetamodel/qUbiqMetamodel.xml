                    <picture sizex="110" sizey="40">
                        <rectangle fill="#ffffff" stroke-style="solid" stroke="#000000" y1="0" stroke-width="0" x1="0" y2="39" fill-style="none" x2="109"/>
                    </picture>
                    <labels>
                        <label x="40a" y="10a" textBinded="name"/>
