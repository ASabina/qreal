<?xml version='1.0' encoding='utf-8'?>
<metamodel xmlns="http://schema.real.com/schema/">
    <include>kernel_metamodel.xml</include>
    <include>basicbehaviors_metamodel.xml</include>
    <namespace>UML 2.0</namespace>
    <diagram name="Actions">
        <graphicTypes>
            <node name="Basic Actions Diagram::Accept Event Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Action"/>
                    </generalizations>
                    <properties>
                        <property type="bool" name="isUnmarshall" />
						<property type="Basic Actions Diagram::Trigger" name="trigger" />
						<property type="Basic Actions Diagram::Output Pin" name="result" />
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Action">
                <logic>
                    <generalizations>
                            <parent parentName="Kernel Diagram::Named Element"/>
                    </generalizations>
					<properties>
					  <property type="Basic Actions Diagram::Input Pin" name="input"/>
					  <property type="Basic Actions Diagram::Output Pin" name="output"/>
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Trigger">
                <!-- "from Communications" -->
                <logic>
                    <generalizations>
                            <parent parentName="Kernel Diagram::Named Element"/>
                    </generalizations>
					<properties>
						<property type="Basic Actions Diagram::Event" name="event" />
						<property type="Basic Actions Diagram::Accept Event Action" name="trigger" />
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Output Pin">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Pin"/>
                    </generalizations>
					<properties>
						<property type="Basic Actions Diagram::Accept Event Action" name="result" />
						<property type="Basic Actions Diagram::Action" name="output" />
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Input Pin">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Pin"/>
                    </generalizations>
					<properties>
						<property type="Basic Actions Diagram::Action" name="input" />
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Event">
                <!-- "from Communications" -->
                <logic>
                    <generalizations>
                            <parent parentName="Kernel Diagram::Packageable Element"/>
                    </generalizations>
					<properties>
						<property type="Basic Actions Diagram::Trigger" name="event" />
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Pin">
                <logic>
                    <generalizations>
                            <parent parentName="Kernel Diagram::Typed Element"/>
                            <parent parentName="Kernel Diagram::Multiplicity Element"/>
                    </generalizations>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Action Input Pin">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Input Pin"/>
                    </generalizations>
					<properties>
						<property type="Basic Actions Diagram::Action" name="fromAction" />
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Value Specification Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Action"/>
                    </generalizations>
					<properties>
						<property type="Kernel Diagram::Value Specification" name="value" />
						<property type="Kernel Diagram::Output Pin" name="result" />
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Add Variable Value Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Write Variable Action"/>
                    </generalizations>
                    <properties>
                        <property type="bool" name="isReplaceAll">
                            <default>false</default>
                        </property>
						<property type="Basic Actions Diagram::Input Pin" name="insertAt"/>
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Write Variable Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Variable Action"/>
                    </generalizations>
					<properties>
						<property type="Basic Actions Diagram::Input Pin" name="value" />
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Variable Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Action"/>
                    </generalizations>
					<properties>
						<property type="Basic Actions Diagram::Variable" name="variable" />
					</properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Variable">
                <logic>
                    <generalizations>
                            <parent parentName="Kernel Diagram::Multiplicity Element"/>
                            <parent parentName="Kernel Diagram::Typed Element"/>
                    </generalizations>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Call Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Invocation Action"/>
                    </generalizations>
                    <properties>
                        <property type="bool" name="isSynchronous">
                            <default>true</default>
                        </property>
						<property type="Basic Actions Diagram::Output Pin" name="result"/>
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Call Behavior Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Call Action"/>
                    </generalizations>
                    <properties>
                            <property type="Basic Behavior Diagram::Behavior" name="behavior"/>
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Call Operation Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Call Action" />
                    </generalizations>
                        <properties>
                            <property type="Basic Actions Diagram::Input Pin" name="target"/>
                            <property type="Kernel Diagram::Operation" name="operation"/>
                        </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Send Object Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Invocation Action"/>
                    </generalizations>
                    <properties>
                            <property type="Basic Actions Diagram::Input Pin" name="request"/>
                            <property type="Basic Actions Diagram::Input Pin" name="target"/>
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Invocation Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Action"/>
                    </generalizations>
                    <properties>
                            <property type="Basic Actions Diagram::Input Pin" name="argument"/>
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::SendSignal Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Invocation Action"/>
                    </generalizations>
                    <properties>
                            <property type="Basic Actions Diagram::Signal" name="signal"/>
                            <property type="Basic Actions Diagram::Input Pin" name="target"/>
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Signal">
                <!-- "from Communication" -->
                <logic>
                    <generalizations>
                            <parent parentName="Kernel Diagram::Classifier"/>
                    </generalizations>
                    <properties>
                            <property type="Kernel Diagram::Property" name="ownedAttribute"/>
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Unmarshall Action">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Action"/>
                    </generalizations>
                    <properties>
                            <property type="Basic Actions Diagram::Input Pin" name="object"/>
                            <property type="Kernel Diagram::Classifier" name="unmarshallType"/>
                            <property type="Basic Actions Diagram::Output Pin" name="result"/>
                    </properties>
                </logic>
            </node>
            <node name="Basic Actions Diagram::Value Pin">
                <logic>
                    <generalizations>
                            <parent parentName="Basic Actions Diagram::Input Pin"/>
                    </generalizations>
                    <properties>
                            <property type="Kernel Diagram::Value Specification" name="value"/>
                    </properties>
                </logic>
            </node>

        </graphicTypes>
    </diagram>
</metamodel>
