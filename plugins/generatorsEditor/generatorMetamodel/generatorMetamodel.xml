                    </picture>
                    <labels>
                        <label x="5" y="3" textBinded="Semantic / ##elementName##"/>
                    </labels>
                    <ports>
                        <linePort type="NonTyped">
                            <start startx="-1" starty="10"/>
                            <end endx="0" endy="89"/>
                        </linePort>
                        <linePort type="NonTyped">
                            <start startx="149" starty="10"/>
                            <end endx="150" endy="89"/>
                        </linePort>
                        <linePort type="NonTyped">
