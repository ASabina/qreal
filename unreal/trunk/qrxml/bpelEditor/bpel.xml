<?xml version='1.0' encoding='utf-8'?>
<metamodel xmlns="http://schema.real.com/schema/" >
 <include>../commonXml/kernel_metamodel.xml</include>
 <namespace>UML 2.1</namespace>
 <diagram name="BPEL" >
 <graphicTypes>

	<edge id="bplRelationShip" name="Relationship">
    <graphics> <lineType type="solidLine"/> </graphics>
    <logic>
    <properties>
     <property type="string" name="suppressJoinFailure" />
     <property type="string" name="comment" />
    </properties>
      <associations beginType="no_arrow" endType="filled_rhomb">
        <association beginName="Element" endName="Element"/>
      </associations>
      <generalizations> <parent parentName="Kernel::Association"/> </generalizations>
    </logic>
  </edge>

  <node id="bplAbstract" name="bplAbstract" >
   <logic>
    <properties>
		 <property type="bool" name="isAbstract"> <default>false</default> </property>
     <property type="string" name="suppressJoinFailure" />
     <property type="string" name="comment" />
     <property type="string" name="joinCondition" />
     <property type="string" name="createInstance" />
     <property type="string" name="operation" />
     <property type="string" name="partnerLink" />
     <property type="string" name="portType" />
     <property type="string" name="variable" />
    </properties>
<!--    <generalizations> <parent parentName="Kernel::Class" /> </generalizations>-->
   </logic>
  </node>

  <node id="bplReply" name="Reply" >
   <graphics>
     <picture sizex="70" sizey="100" >
      <text i="0" font-fill="#0000ff" y1="10a" b="0" x1="10a" font-name="Helvetica" u="0" font-size="15a" >bplReply</text>
			<line fill="#000000" stroke-style="solid" stroke="#00F000" y1="20a" x1="0a" y2="20a" stroke-width="2" x2="100%" fill-style="solid"/>
			<line fill="#000000" stroke-style="solid" stroke="#00F000" y1="100%" x1="0a" y2="100%" stroke-width="2" x2="100%" fill-style="solid"/>
			<line fill="#000000" stroke-style="solid" stroke="#00F000" y1="20a" x1="0a" y2="100%" stroke-width="2" x2="0a" fill-style="solid"/>
			<line fill="#000000" stroke-style="solid" stroke="#00F000" y1="20a" x1="100%" y2="100%" stroke-width="2" x2="100%" fill-style="solid"/>
     </picture>
		 <labels> <label x="30" y="30" textBinded="name"/> </labels>
     <ports>
      <linePort> <start startx="0a" starty="20a" />  <end endx="0a" endy="100%" /> </linePort>
      <linePort> <start startx="100%" starty="20a" />  <end endx="100%" endy="100%" /> </linePort>
      <linePort> <start startx="0a" starty="20a" />  <end endx="100%" endy="20a" /> </linePort>
      <linePort> <start startx="0a" starty="100%" />  <end endx="100%" endy="100%" /> </linePort>
     </ports>
   </graphics>
   <logic>
    <properties>
		 <property type="string" name="faultName" />
     <property type="string" name="operation" />
    </properties>
    <generalizations> <parent parentName="BPEL::bplAbstract" /> </generalizations>
   </logic>
  </node>

  <node id="bplReceive" name="Receive" >
   <graphics>
     <picture sizex="70" sizey="100" >
      <text i="0" font-fill="#0000ff" y1="10a" b="0" x1="10a" font-name="Helvetica" u="0" font-size="15a" >bplReceive</text>
			<line fill="#000000" stroke-style="solid" stroke="#00F000" y1="20a" x1="0a" y2="20a" stroke-width="2" x2="100%" fill-style="solid"/>
			<line fill="#000000" stroke-style="solid" stroke="#00F000" y1="100%" x1="0a" y2="100%" stroke-width="2" x2="100%" fill-style="solid"/>
			<line fill="#000000" stroke-style="solid" stroke="#00F000" y1="20a" x1="0a" y2="100%" stroke-width="2" x2="0a" fill-style="solid"/>
