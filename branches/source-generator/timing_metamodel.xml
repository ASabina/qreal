<?xml version="1.0" encoding="utf-8"?>
<metamodel xmlns="http://schema.real.com/schema/">
  <include></include>
  <namespace>UML 2.1</namespace>
  <diagram name="TIMING DIAGRAM">
    <node name="Interaction" id="tmnnInteraction">
      <graphics>
        <view>
          <svg_shape>
            <svg:svg width="100" xmlns:xlink="http://www.w3.org/1999/xlink" height="72" version="1.0">
              <svg:rect x="1" y="1" width="98" height="60" style="fill:white;stroke:black;stroke-width:2;fill-opacity:0;" />
              <svg:text x="6" y="16" font-weight="bold" style="font-size:12pt;fill:#000000;">sd</svg:text>
            </svg:svg>
          </svg_shape>
          <repo_info>
            <html:html xmlns:html="http://www.w3.org/html/">
              <html:text_from_repo name="name"/>
            </html:html>
            <html:html xmlns:html="http://www.w3.org/html/">
              <html:line x1="1" y1="21" x2="41" y2="21" stroke="black" stroke-width="2" />
              <html:line x1="41" y1="21" x2="51" y2="11" stroke="black" stroke-width="2" />
              <html:line x1="51" y1="11" x2="51" y2="1" stroke="black" stroke-width="2" />
              <!--просто тут имя диаграммы, написанное рядом с sd может не влезть в отведенное место, а потому, надо уметь его растягивать-->
            </html:html>
          </repo_info>
        </view>
      </graphics>
      <logic>
        <generalizations>
          <generalization>
            <parent parent_id="bbnBehavior" />
            <parent parent_id="tmnnInteractionFragment" />
          </generalization>
        </generalizations>
        <containers>
          <container>
            <contain idref="tmnnLifeline" role="lifeline" />
            <contain idref="tmnnaMessage" role="message" />
            <contain idref="tmnnInteractionFragment" role="fragment" />
          </container>
          <container>
            <contained_by idref="tmnnInteractionFragment" role="lastInteraction" />
          </container>
        </containers>
        <properties>
          <property name="name" type="string" />
        </properties>
      </logic>
    </node>

    <node name="InteractionFragment" id="tmnnInteractionFragment">
      <logic>
        <generalizations>
          <generalization>
            <parent parent_id="krnnNamedElement" />
          </generalization>
        </generalizations>
        <containers>
          <container>
            <contain idref="tmnnLifeline" role="covered" />
            <contain idref="tmnaGeneralOrdering" role="generalOrdering" />
            <contain idref="tmnnInteraction" role="enclosingInteraction" />
          </container>
          <container>
            <contained_by idref="tmnnInteraction" role="interaction" />
          </container>
        </containers>
      </logic>
    </node>

    <node name="StateInvariant" id="tmnnStateInvariant">
      <graphics>
        <view>
          <!--и как оно рисуется?-->
          <repo_info>
            <html:html xmlns:html="http://www.w3.org/html/">
              <html:text_from_repo name="name"/>
            </html:html>
          </repo_info>
        </view>
      </graphics>
      <logic>
        <generalizations>
          <generalization>
            <parent parent_id="tmnnInteractionFragment" />
          </generalization>
        </generalizations>
        <containers>
          <container>
            <contained_by idref="tmnnLifeline" />
          </container>
        </containers>
        <properties>
          <property name="name" type="string" />
        </properties>
      </logic>
    </node>

    <node name="Lifeline" id="tmnnLifeline">
      <graphics>
        <view>
          <!--надо рисовать жирным-->
          <line_type type="solidLine" />
        </view>
      </graphics>
      <logic>
        <generalizations>
          <generalization>
            <parent parent_id="krnnNamedElement" />
          </generalization>
        </generalizations>
        <containers>
          <container>
            <contained_by idref="tmnnInteraction" role="interaction" />
            <contained_by idref="tmnnInteractionFragment" role="coveredBy" />
          </container>
        </containers>
      </logic>
    </node>

    <edge name="Message" id="tmnaMessage">
      <graphics>
        <view>
          <line_type type="solidLine" />
          <end_type type="arrow" />
          <repo_info>
            <html:html xmlns:html="http://www.w3.org/html/">
              <html:text_from_repo name="name"/>
            </html:html>
          </repo_info>
        </view>
      </graphics>
      <logic>
        <generalizations>
          <generalization>
            <parent parent_id="krnnNamedElement" />
          </generalization>
        </generalizations>
        <containers>
          <container>
            <contained_by idref="tmnnInteraction" />
          </container>
        </containers>
        <properties>
          <property name="name" type="string" />
        </properties>
      </logic>
    </edge>
  </diagram>
</metamodel>
